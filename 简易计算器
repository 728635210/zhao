v0
#include<stdio.h>
int main(void)
{
	char s[] = "3+4";
	int x;
	x = s[0] - '0';
	x += s[2] - '0';
	printf("%d\n",x);

	return 0;
}

v1
#include<stdio.h>
int main(void)
{
	char s[] = "1+1+2+3+4+4";
	int i,x;
	x = s[0] - '0';
	for(i = 1;s[i] != '\0';i++)
	{
		if(s[i] == '+')
		{
			i++;
			x += s[i] - '0';
		}
		else if(s[i] == '-')
		{
			i++;
			x = x - (s[i] - '0');
		}
	}	
	printf("%d\n",x);
	return 0;
}


v2
#include<stdio.h>
int main(void)
{
	char s[] = "1*2/2*2*4*4";
	int i,x;
	x = s[0] - '0';
	for(i = 1;s[i] != '\0';i++)
	{
		if(s[i] == '*')
		{
			i++;
			x = x*(s[i] - '0');
		}
		else if(s[i] == '/')
		{
			i++;
			x = x/(s[i] - '0');
		}
	}	
	printf("%d\n",x);
	return 0;
}

v3
#include<stdio.h>
int main(void)
{
	char s[] = "4+1*2-2/2+1+1";
	int i,x;
	x = s[0] - '0';
	for(i = 1;s[i] != '\0';i++)
	{
		if(s[i + 2] == '*')
		{
			if(s[i] == '+')
			{
				i++;
				x += (s[i] - '0')*(s[i+2] - '0');
				i = i + 2;
			}
			else if(s[i] == '-')
			{
				i++;
				x = x - (s[i] - '0')*(s[i+2] - '0');
				i = i + 2;
			}
		}
		else if(s[i + 2] == '/')
		{
			if(s[i] == '+')
			{
				i++;
				x += (s[i] - '0')/(s[i+2] - '0');
				i = i + 2;
			}
			else if(s[i] == '-')
			{
				i++;
				x = x - (s[i] - '0')/(s[i+2] - '0');
				i = i + 2;
			}
		}
		else if(s[i + 2] == '+')
		{
			if(s[i] == '+')
			{
				i++;
				x += (s[i] - '0')+(s[i+2] - '0');
				i = i + 2;
			}
			else if(s[i] == '-')
			{
				i++;
				x = x - (s[i] - '0')+(s[i+2] - '0');
				i = i + 2;
			}
		}
		else if(s[i + 2] == '-')
		{
			if(s[i] == '+')
			{
				i++;
				x += (s[i] - '0')-(s[i+2] - '0');
				i = i + 2;
			}
			else if(s[i] == '-')
			{
				i++;
				x = x - (s[i] - '0')-(s[i+2] - '0');
				i = i + 2;
			}
		}
	}	
	printf("%d\n",x);
	return 0;
}
